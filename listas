#listas 1
def listas1():
    print('Digame cuantas palabras tiene la lista:')
    numeroPal=int(input())
    palabras=[]
    if numeroPal != 0:
        for i in range(numeroPal):
            print('Digite la palabra numero %s: ' % (i+1))
            p=str(input())
            palabras.append(p)


        print(palabras)

    else :
        print('Imposible!')



listas1()
